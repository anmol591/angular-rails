<!Doctype html>
<html>
<head>
	<title></title>
</head>
<body>
	<div>
		<% if flash[:notice]%>
		<p><%= flash[:notice]  %></p>
		<% end %>
		<%= form_for :user, :url=>{:action=>'user_sign_in_form'}, :method=>'post' do |f| %>
		<p>Email:<%= f.text_field :email %></p>
		<p>Password:<%= f.password_field :password %></p>
		<p><%= f.submit 'submit'%></p>
		<% end %>
	</div>
</body>

</html>
